
<section class="blog-posts">
    {{#each posts}}
    <header class="blog-header">
        {{!-- Insert the post's title --}}
        <h1>{{title}}</h1>
    </header>
    <main>
        <section class="post-card">
            <div class="post-content">
                <p>{{content}}</p>
            </div>
            <div class="post-meta">
                <span class="post-date">{{date}}</span>
            </div>
        </section>
    </main>
    {{/each}}
</section>

<script src="/js/dashboard.js"></script>
